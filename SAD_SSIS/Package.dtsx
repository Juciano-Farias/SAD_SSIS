                <property
                  dataType="System.String"
                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  J.ID as JogoID, 
  J.CompeticaoID, 
  J.EquipaCasaID, 
  J.EquipaForaID, 
  J.EstadioID, 
  J.Data,
  casa.AmarelosCasa, 
  fora.AmarelosFora, 
  fora.VermelhosFora, 
  casa.VermelhosCasa,
  casa.GolsCasa, 
  fora.GolsFora, 
  casa.AssistenciaCasa, 
  fora.AssistenciaFora
FROM Jogos J
LEFT JOIN (
  SELECT 
    JogoID, 
    EquipaID, 
    SUM(Amarelos) as AmarelosCasa, 
    SUM(CAST(Vermelho AS INT)) as VermelhosCasa, 
    SUM(Golos) as GolsCasa, 
    SUM(Assistencias) as AssistenciaCasa
  FROM FichasJogo
  WHERE EquipaID IN (SELECT EquipaCasaID FROM Jogos)
  GROUP BY JogoID, EquipaID
) casa ON casa.EquipaID = J.EquipaCasaID AND casa.JogoID = J.ID
LEFT JOIN (
  SELECT 
    JogoID, 
    EquipaID, 
    SUM(Amarelos) as AmarelosFora, 
    SUM(CAST(Vermelho AS INT)) as VermelhosFora, 
    SUM(Golos) as GolsFora, 
    SUM(Assistencias) as AssistenciaFora
  FROM FichasJogo
  WHERE EquipaID IN (SELECT EquipaForaID FROM Jogos)
  GROUP BY JogoID, EquipaID
) fora ON fora.EquipaID = J.EquipaForaID AND fora.JogoID = J.ID
ORDER BY J.ID;</property>
