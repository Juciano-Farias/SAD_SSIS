      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0A7221B1-2DB0-402F-8FA4-3B8BDB26E7F9}"
          SQLTask:SqlStatementSource="-- Verifica se a tabela temporária de números existe e a cria se necessário&#xA;IF OBJECT_ID('tempdb..#Numbers') IS NULL&#xA;BEGIN&#xA;    CREATE TABLE #Numbers (Number INT PRIMARY KEY);&#xA;&#xA;    DECLARE @i INT = 0;&#xA;    WHILE @i &lt; 50000 BEGIN  -- número suficiente para cobrir o período desde 1900 até hoje e além&#xA;        INSERT INTO #Numbers VALUES (@i);&#xA;        SET @i = @i + 1;&#xA;    END&#xA;END&#xA;&#xA;-- Verifica se a tabela DimTempo existe e a cria se necessário&#xA;IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[DimTempo]') AND type in (N'U'))&#xA;BEGIN&#xA;    CREATE TABLE [dbo].[DimTempo] (&#xA;        DataId INT PRIMARY KEY,&#xA;        Data DATE,&#xA;        Dia INT,&#xA;        Mes INT,&#xA;        Ano INT,&#xA;        Semana INT,&#xA;        Trimestre INT&#xA;    );&#xA;END&#xA;&#xA;-- Encontra a última data na DimTempo para começar a inserção a partir do dia seguinte&#xA;DECLARE @LastDate DATE;&#xA;SELECT @LastDate = MAX(Data) FROM [dbo].[DimTempo];&#xA;&#xA;-- Insere novas datas a partir do dia seguinte da última data até hoje&#xA;INSERT INTO [dbo].[DimTempo] (DataId, Data, Dia, Mes, Ano, Semana, Trimestre)&#xA;SELECT&#xA;    CONVERT(INT, REPLACE(CONVERT(VARCHAR, DateAdd(day, Number, '1900-01-01'), 112), '-', '')) AS DataId,&#xA;    DateAdd(day, Number, '1900-01-01') AS Data,&#xA;    DAY(DateAdd(day, Number, '1900-01-01')) AS Dia,&#xA;    MONTH(DateAdd(day, Number, '1900-01-01')) AS Mes,&#xA;    YEAR(DateAdd(day, Number, '1900-01-01')) AS Ano,&#xA;    DATEPART(ISO_WEEK, DateAdd(day, Number, '1900-01-01')) AS Semana,&#xA;    (MONTH(DateAdd(day, Number, '1900-01-01')) - 1) / 3 + 1 AS Trimestre&#xA;FROM #Numbers&#xA;WHERE DateAdd(day, Number, '1900-01-01') &gt; ISNULL(@LastDate, '1899-12-31')&#xA;  AND DateAdd(day, Number, '1900-01-01') &lt;= GETDATE();&#xA;&#xA;-- Limpa a tabela de números após uso&#xA;DROP TABLE #Numbers;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
